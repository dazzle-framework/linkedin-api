{
    "name": "dazzlesoftware/linkedin",
    "type": "library",
    "description": "Dazzle Linkedin Package",
    "keywords": ["dazzle", "framework", "linkedin"],
    "homepage": "https://github.com/dazzle-framework/linkedin-api",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/oauth1": "^1.0",
        "dazzlesoftware/input": "^1.0",
        "dazzlesoftware/http": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
    	"psr-4": {
    		"Dazzle\\Linkedin\\": "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Linkedin\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}

